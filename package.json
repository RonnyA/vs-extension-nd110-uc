{
  "name": "nd-110-microcode",
  "displayName": "ND-110 MicroCode",
  "description": "Syntax highlighting for ND-110 MicroCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "uc",
      "aliases": ["ND-110 MicroCode", "uc"],
      "extensions": [".uc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "uc",
      "scopeName": "source.uc",
      "path": "./syntaxes/uc.tmLanguage.json"
    }]
  }
}
